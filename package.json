{
  "name": "barbershop-schedule-management",
  "version": "1.0.0",
  "description": "Microservice responsible for managing schedules on the Barbershop platform.",
  "main": "dist/server.js",
  "scripts": {
    "build": "gulp build",
    "build:doc": "typedoc --options typedoc.json",
    "start:dev": "gulp dev",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "nyc --clean --reporter=text-summary mocha test/**/*.spec.ts",
    "test:unit": "nyc --clean --all --reporter=text-summary mocha test/unit/**/*.spec.ts",
    "test:integration": "nyc --clean --all --reporter=text-summary mocha test/integration/**/*.spec.ts",
    "test:cov": "nyc --clean --all --reporter=html --reporter=text mocha test/**/*.spec.ts",
    "start": "node dist/server.js",
    "pretest": "rimraf .nyc_output .cache coverage dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KaiqueIvo04/barbershop-schedule-management.git"
  },
  "bugs": {
    "url": "https://github.com/KaiqueIvo04/barbershop-schedule-management/issues"
  },
  "homepage": "https://github.com/KaiqueIvo04/barbershop-schedule-management#readme",
  "keywords": [
    "schedule-management",
    "microservice",
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "KaiqueIvo04",
  "license": "Apache-2.0",
  "nyc": {
    "include": [
        "**/*.tsx",
        "**/*.ts"
      ],
      "exclude": [
        "**/*.d.ts",
        "test/*",
        "config/*"
      ],
      "extension": [
        ".ts",
        ".tsx"
      ],
      "reporter": [
        "text-summary",
        "html"
      ],
      "all": true
  },
  "dependencies": {
    "amqp-client-node": "^1.0.11",
    "axios": "^0.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "express-interceptor": "^1.2.0",
    "helmet": "^5.0.1",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.6",
    "mongoose-field-encryption": "^4.0.3",
    "morgan": "^1.10.0",
    "on-finished": "^2.3.0",
    "query-strings-parser": "^2.1.10",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "winston": "^3.4.0",
    "winston-daily-rotate-file": "^4.5.5",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/mocha": "^9.0.0",
    "@types/morgan": "^1.9.3",
    "@types/swagger-ui-express": "^4.1.3",
    "chai": "^4.3.4",
    "gulp": "^4.0.2",
    "gulp-nodemon": "^2.5.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^6.0.0-alpha.1",
    "mocha": "^9.1.3",
    "nyc": "^17.0.0",
    "supertest": "^6.2.0",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  }
}
